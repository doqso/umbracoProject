@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;

@{
    Layout = "_Layout.cshtml";
    Console.WriteLine(Model.Children.FirstOrDefault());

    var articles = (Model.IsHomePage)
    ? Model.Children.SelectMany(c => c.Children)
    : Model.Children;
}

@await Html.PartialAsync("blog/Header")
@await Html.PartialAsync("blog/News")

@await Html.PartialAsync("blog/Articles", articles)